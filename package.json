{
  "name": "tree-sitter-brewfile",
  "version": "1.0.0",
  "description": "Tree-sitter grammar for Homebrew Brewfiles",
  "main": "bindings/node",
  "repository": {
    "type": "git",
    "url": "https://github.com/arcaartem/tree-sitter-brewfile.git"
  },
  "author": "Arca Artem",
  "license": "MIT",
  "keywords": [
    "tree-sitter",
    "brewfile",
    "homebrew",
    "parser",
    "syntax-highlighting"
  ],
  "dependencies": {
    "nan": "^2.17.0"
  },
  "devDependencies": {},
  "scripts": {
    "generate": "tree-sitter generate",
    "build": "tree-sitter generate && node-gyp rebuild",
    "test": "tree-sitter test"
  },
  "tree-sitter": [
    {
      "scope": "source.brewfile",
      "file-types": [
        "Brewfile"
      ]
    }
  ]
}
